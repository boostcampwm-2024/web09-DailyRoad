{
  "name": "backend",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "test": "jest -i --config jest.config.ts",
    "test:cov": "npm run test -- --coverage",
    "test:e2e": "jest -i --config test/config/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --write **/*.{ts,tsx}",
    "format": "yarn lint && yarn prettier",
    "format-check": "eslint \"{src,apps,libs,test}/**/*.ts\" && prettier --check **/*.{ts,tsx}"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "cookie-parser": "^1.4.6",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/schedule": "^4.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "google-auth-library": "^9.14.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "reflect-metadata": "^0.2.0",
    "nestjs-pino": "^4.1.0",
    "pino-http": "^10.3.0",
    "pino-socket": "^7.4.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0",
    "@nestjs/elasticsearch": "^10.0.2",
    "@elastic/elasticsearch": "^8.16.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@testcontainers/mysql": "^10.14.0",
    "@types/express": "^4.17.17",
    "@types/cookie-parser": "^1.4.6",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "testcontainers": "^10.14.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "pino-pretty": "^13.0.0"
  }
